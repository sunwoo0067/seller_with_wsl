# 환경 설정
ENV=development
DEBUG=true
DRY_RUN=false
LOG_LEVEL=INFO
LOG_FILE=logs/dropshipping.log
SCHEDULER_ENABLED=true
SCHEDULER_TIMEZONE=Asia/Seoul
CACHE_ENABLED=true
CACHE_TTL=3600

# 데이터베이스 (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# AI 모델
# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key
# Ollama (로컬 모델)
OLLAMA_HOST=http://localhost:11434

# 공급사 API
# 도매매/도매꾹
DOMEME_API_KEY=your-domeme-api-key
DOMEME_API_URL=https://api.domeggook.com/open/v4.1/search/searchProductList.do

# 오너클랜
OWNERCLAN_API_KEY=your-ownerclan-api-key
OWNERCLAN_API_URL=https://api.ownerclan.com/graphql

# 젠트레이드
ZENTRADE_API_KEY=your-zentrade-api-key
ZENTRADE_API_URL=https://api.zentrade.co.kr

# 마켓플레이스 API
# 쿠팡
COUPANG_ACCESS_KEY=your-coupang-access-key
COUPANG_SECRET_KEY=your-coupang-secret-key
COUPANG_VENDOR_ID=your-vendor-id

# 11번가
ELEVENST_API_KEY=your-11st-api-key
ELEVENST_API_SECRET=your-11st-secret
ELEVENST_SELLER_ID=your-seller-id

# 네이버 스마트스토어
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret
NAVER_ACCESS_TOKEN=your-access-token
NAVER_REFRESH_TOKEN=your-refresh-token

# G마켓/옥션
GMARKET_SELLER_ID=your-gmarket-seller-id
GMARKET_API_KEY=your-gmarket-api-key

# 모니터링
# Slack 웹훅 (선택사항)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# 이메일 알림 (선택사항)
EMAIL_ENABLED=false
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
EMAIL_FROM=noreply@yourdomain.com
EMAIL_TO=admin@yourdomain.com

# 대시보드
DASHBOARD_HOST=0.0.0.0
DASHBOARD_PORT=8080

# API 서버 (향후 구현)
API_HOST=0.0.0.0
API_PORT=8000
API_CORS_ORIGINS=["http://localhost:3000", "https://yourdomain.com"]

# 보안
SECRET_KEY=your-secret-key-for-jwt-and-encryption
API_KEY_SALT=your-api-key-salt

# 파일 경로
LOCAL_DATA_PATH=./data
LOCAL_UPLOAD_PATH=./uploads

# 배치 처리 설정
BATCH_SIZE=50
MAX_PRODUCTS_PER_SUPPLIER=1000
MAX_CONCURRENT_UPLOADS=5

# AI 처리 설정
AI_MONTHLY_BUDGET=50.00
AI_DEFAULT_MODEL=gemma-3b
AI_PROCESS_ENABLED=true

# 재고 관리
LOW_STOCK_THRESHOLD=10
STOCK_SYNC_INTERVAL=3600
SAFETY_STOCK_PERCENTAGE=20

# 가격 정책
DEFAULT_MARGIN_PERCENTAGE=30
MIN_MARGIN_PERCENTAGE=15
MAX_MARGIN_PERCENTAGE=50
PRICE_ROUND_TO=100

# 배송 추적
DELIVERY_CHECK_INTERVAL=3600
DELIVERY_API_TIMEOUT=30