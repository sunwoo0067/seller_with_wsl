# 환경 설정
ENV=development  # development, staging, production

# 로깅
LOG_LEVEL=INFO
LOG_FILE=logs/dropshipping.log

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# 공급사 API
## Domeme (도매매)
DOMEME_API_KEY=your-domeme-api-key
DOMEME_API_URL=https://api.domeggook.com/open/v4.1/search/searchProductList.do

## Ownerclan
OWNERCLAN_API_KEY=your-ownerclan-api-key
OWNERCLAN_API_URL=https://api.ownerclan.com/graphql

## Zentrade
ZENTRADE_API_KEY=your-zentrade-api-key
ZENTRADE_API_URL=https://api.zentrade.com

# 마켓플레이스 API
## 쿠팡
COUPANG_ACCESS_KEY=your-coupang-access-key
COUPANG_SECRET_KEY=your-coupang-secret-key
COUPANG_VENDOR_ID=your-vendor-id

## 11번가
ELEVENST_API_KEY=your-11st-api-key

## 네이버 스마트스토어
SMARTSTORE_CLIENT_ID=your-smartstore-client-id
SMARTSTORE_CLIENT_SECRET=your-smartstore-client-secret

# AI 모델
## Gemini
GEMINI_API_KEY=your-gemini-api-key

## Ollama (로컬)
OLLAMA_HOST=http://localhost:11434

# 모니터링
## Slack Webhook
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# 스케줄러
SCHEDULER_ENABLED=false
SCHEDULER_TIMEZONE=Asia/Seoul

# 개발 설정
DEBUG=true
DRY_RUN=false

# 캐시
CACHE_ENABLED=true
CACHE_TTL=3600  # seconds

# 파일 저장 경로 (Supabase 사용 전 로컬 개발용)
LOCAL_DATA_PATH=./data
LOCAL_UPLOAD_PATH=./uploads